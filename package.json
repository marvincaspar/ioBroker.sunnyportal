{
  "name": "iobroker.sunnyportal",
  "version": "0.1.5",
  "description": "Fetch data from Sunny Portal",
  "author": {
    "name": "Marvin Caspar",
    "email": "info@caspar.dev"
  },
  "contributors": [
    {
      "name": "Marvin Caspar"
    }
  ],
  "homepage": "https://github.com/marvincaspar/ioBroker.sunnyportal",
  "license": "MIT",
  "keywords": [
    "iobroker",
    "smarthome",
    "energy",
    "sunnyportal"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/marvincaspar/ioBroker.sunnyportal"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^2.2.1",
    "flow": "^0.2.3",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8"
  },
  "devDependencies": {
    "@iobroker/testing": "^2.1.0",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/gulp": "^4.0.6",
    "@types/mocha": "^7.0.2",
    "@types/node": "^12.12.34",
    "@types/proxyquire": "^1.3.28",
    "@types/request": "^2.48.4",
    "@types/request-promise-native": "^1.0.17",
    "@types/sinon": "^9.0.0",
    "@types/sinon-chai": "^3.2.4",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "axios": "^0.21.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "gulp": "^4.0.2",
    "mocha": "^7.1.1",
    "nyc": "^15.0.1",
    "prettier": "^2.0.2",
    "proxyquire": "^2.1.3",
    "rimraf": "^3.0.2",
    "sinon": "^8.1.1",
    "sinon-chai": "^3.5.0",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  },
  "main": "build/main.js",
  "scripts": {
    "prebuild": "rimraf ./build",
    "build:ts": "tsc -p tsconfig.build.json",
    "build": "npm run build:ts",
    "watch:ts": "tsc -p tsconfig.build.json --watch",
    "watch": "npm run watch:ts",
    "test:ts": "mocha --opts test/mocha.custom.opts",
    "test:package": "mocha test/package --exit",
    "test:unit": "mocha test/unit --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:ts && npm run test:package",
    "coverage": "nyc npm run test:ts",
    "lint": "eslint --ext .ts src"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.test.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "bugs": {
    "url": "https://github.com/marvincaspar/ioBroker.sunnyportal/issues"
  },
  "readmeFilename": "README.md"
}
